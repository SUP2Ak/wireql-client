{
  "name": "wireql-client",
  "version": "1.0.3",
  "description": "Client TypeScript officiel pour WireQL - Base de donnÃ©es as-a-Service avec WebSocket et HTTP",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:types": "tsc --project tsconfig.types.json",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build && npm run test",
    "docs": "typedoc src/index.ts",
    "publish:dry": "npm pack --dry-run",
    "publish:script": "bash scripts/publish.sh",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "keywords": [
    "wireql",
    "mysql",
    "mariadb",
    "database",
    "websocket",
    "api",
    "client",
    "typescript",
    "sql",
    "orm",
    "query-builder",
    "real-time"
  ],
  "author": "WireQL Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SUP2Ak/wireql-client.git"
  },
  "bugs": {
    "url": "https://github.com/SUP2Ak/wireql-client/issues"
  },
  "homepage": "https://github.com/SUP2Ak/wireql-client#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0",
    "ws": "^8.16.0",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0",
    "typedoc": "^0.25.0",
    "rimraf": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
